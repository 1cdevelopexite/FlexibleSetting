	Запрос.УстановитьПараметр("Документ"   , Источник);
	РезультатЗапроса = Запрос.Выполнить();
	Результат = РезультатЗапроса.Выгрузить();

	Результат.Колонки.Добавить("PRICE"		          , Новый ОписаниеТипов("Строка"));
	Результат.Колонки.Добавить("AMOUNT"		          , Новый ОписаниеТипов("Строка"));
	Результат.Колонки.Добавить("DELIVEREDQUANTITY"    , Новый ОписаниеТипов("Строка"));  
	Результат.Колонки.Добавить("QUANTITYOFCUINTU"  	  , Новый ОписаниеТипов("Строка"));
	Результат.Колонки.Добавить("VAT"			      , Новый ОписаниеТипов("Строка"));
	Результат.Колонки.Добавить("RETURNQUANTITYUNIT"   , Новый ОписаниеТипов("Строка"));
	Результат.Колонки.Добавить("DELIVEREDQUANTITYUNIT", Новый ОписаниеТипов("Строка"));
	Результат.Колонки.Добавить("RETURNQUANTITY"       , Новый ОписаниеТипов("Строка"));

	СоответствиеЕдИзм = Новый Соответствие;
	СоответствиеЕдИзм.Вставить("кг"  , "KGM");
	СоответствиеЕдИзм.Вставить("кг." , "KGM");
	СоответствиеЕдИзм.Вставить("м"   , "MTR"); 
	СоответствиеЕдИзм.Вставить("м."  , "MTR");
	СоответствиеЕдИзм.Вставить("гол" , "ZHD");
	СоответствиеЕдИзм.Вставить("гол.", "ZHD");
	СоответствиеЕдИзм.Вставить("г"   , "GRM");
	СоответствиеЕдИзм.Вставить("г."  , "GRM");      

	Для Каждого Строка Из Результат Цикл
		Строка.PRICE 	         = Формат(Строка.Цена,  "ЧДЦ=2; ЧРД=.; ЧН=; ЧГ=0");
		Строка.AMOUNT 	         = Формат(Строка.Сумма, "ЧДЦ=2; ЧРД=.; ЧН=; ЧГ=0");
		Строка.QUANTITYOFCUINTU  = "0";
		Строка.RETURNQUANTITY 	 = Формат(Строка.Количество   , "ЧДЦ=3; ЧРД=.; ЧН=; ЧГ=0");
     	Строка.DELIVEREDQUANTITY = Формат(Строка.КоличествоДоставлено, "ЧДЦ=3; ЧРД=.; ЧН=; ЧГ=0");
		
		 Если СоответствиеЕдИзм.Получить(Строка.ЕдИзмНаименование) = Неопределено Тогда
			Строка.RETURNQUANTITYUNIT 	 = "PCE";
		 Иначе
		    Строка.RETURNQUANTITYUNIT 	 = СоответствиеЕдИзм[Строка.ЕдИзмНаименование];
		КонецЕсли;    
		
		Если СоответствиеЕдИзм.Получить(Строка.ЕдИзмДоставлено) = Неопределено Тогда
			Строка.DELIVEREDQUANTITYUNIT = "PCE";
		 Иначе
		    Строка.DELIVEREDQUANTITYUNIT = СоответствиеЕдИзм[Строка.ЕдИзмДоставлено];
		КонецЕсли;     	
 	
		
