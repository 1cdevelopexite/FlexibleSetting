				<Value xsi:type="xs:string">Отборы 			 = Новый Структура("ВозвратнаяНакладная", Источник);
ПолученныеПоля   = ЭКОМ_ОбщегоНазначения.ПолучитьПоляЦепочкиДокументов("ЭКОМ_ЦепочкиДокументовПокупателя", "Накладная", Отборы);

Поступление = Документы.ПоступлениеТоваровУслуг.ПустаяСсылка();   
Если ПолученныеПоля.Успешно Тогда 
	Поступление = ПолученныеПоля.Накладная;
КонецЕсли;

Запрос = Новый Запрос;
Запрос.Текст = "ВЫБРАТЬ
               |	ПоступлениеТоваровУслугТовары.Номенклатура КАК Номенклатура,
               |	СУММА(ПоступлениеТоваровУслугТовары.Количество) КАК КоличествоПоступление,
               |	ПоступлениеТоваровУслугТовары.Цена КАК Цена,
               |	ПоступлениеТоваровУслугТовары.ЕдиницаИзмерения.Наименование КАК ЕдиницаИзмеренияНаименование
               |ПОМЕСТИТЬ ПоступлениеТовары
               |ИЗ
               |	Документ.ПоступлениеТоваровУслуг.Товары КАК ПоступлениеТоваровУслугТовары
               |ГДЕ
               |	ПоступлениеТоваровУслугТовары.Ссылка = &amp;Поступление
               |
               |СГРУППИРОВАТЬ ПО
               |	ПоступлениеТоваровУслугТовары.Номенклатура,
               |	ПоступлениеТоваровУслугТовары.Цена,
               |	ПоступлениеТоваровУслугТовары.ЕдиницаИзмерения.Наименование
               |;
               |
               |////////////////////////////////////////////////////////////////////////////////
               |ВЫБРАТЬ
               |	ВозвратТоваровПоставщикуТовары.Ссылка КАК Ссылка,
               |	ВозвратТоваровПоставщикуТовары.Ссылка.ВалютаДокумента.Наименование КАК ВалютаДокумента,
               |	ВозвратТоваровПоставщикуТовары.НомерСтроки КАК НомерСтроки,
               |	ВозвратТоваровПоставщикуТовары.Номенклатура КАК Номенклатура,
               |	ВозвратТоваровПоставщикуТовары.Номенклатура.Артикул КАК НоменклатураАртикул,
               |	ВозвратТоваровПоставщикуТовары.Номенклатура.Представление КАК НоменклатураПредставление,
               |	СУММА(ВозвратТоваровПоставщикуТовары.Количество) КАК Количество,
               |	СУММА(ВозвратТоваровПоставщикуТовары.Сумма) КАК Сумма,
               |	ВозвратТоваровПоставщикуТовары.ЕдиницаИзмерения.Наименование КАК ЕдИзмНаименование,
               |	ВозвратТоваровПоставщикуТовары.Цена КАК Цена,
               |	ВозвратТоваровПоставщикуТовары.СтавкаНДС КАК СтавкаНДС,
               |	""12E"" КАК CODE
               |ПОМЕСТИТЬ ВТ_Товары
               |ИЗ
               |	Документ.ВозвратТоваровПоставщику.Товары КАК ВозвратТоваровПоставщикуТовары
               |ГДЕ
               |	ВозвратТоваровПоставщикуТовары.Ссылка В(&amp;Документ)
               |
               |СГРУППИРОВАТЬ ПО
               |	ВозвратТоваровПоставщикуТовары.Ссылка,
               |	ВозвратТоваровПоставщикуТовары.НомерСтроки,
               |	ВозвратТоваровПоставщикуТовары.Номенклатура.Артикул,
               |	ВозвратТоваровПоставщикуТовары.Номенклатура,
               |	ВозвратТоваровПоставщикуТовары.ЕдиницаИзмерения.Наименование,
               |	ВозвратТоваровПоставщикуТовары.Цена,
               |	ВозвратТоваровПоставщикуТовары.СтавкаНДС,
               |	ВозвратТоваровПоставщикуТовары.Номенклатура.Представление,
               |	ВозвратТоваровПоставщикуТовары.Ссылка.ВалютаДокумента.Наименование
               |;
               |
               |////////////////////////////////////////////////////////////////////////////////
               |ВЫБРАТЬ
               |	ВТ_Товары.Номенклатура КАК Номенклатура,
               |	МАКСИМУМ(ШтрихкодыНоменклатуры.Штрихкод) КАК Штрихкод
               |ПОМЕСТИТЬ ВТ_ШтрихкодыМакс
               |ИЗ
               |	ВТ_Товары КАК ВТ_Товары
               |		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ШтрихкодыНоменклатуры КАК ШтрихкодыНоменклатуры
               |		ПО ВТ_Товары.Номенклатура = ШтрихкодыНоменклатуры.Номенклатура
               |
               |СГРУППИРОВАТЬ ПО
               |	ВТ_Товары.Номенклатура
               |;
               |
               |////////////////////////////////////////////////////////////////////////////////
               |ВЫБРАТЬ
               |	ВТ_Товары.Ссылка КАК Ссылка,
               |	ВТ_Товары.НомерСтроки КАК POSITIONNUMBER,
               |	ВТ_Товары.ЕдИзмНаименование КАК ЕдИзмНаименование,
               |	ВТ_Товары.Номенклатура КАК Номенклатура,
               |	ЕСТЬNULL(ВТ_ШтрихкодыМакс.Штрихкод, ВТ_Товары.НоменклатураАртикул) КАК PRODUCT,
               |	ВТ_Товары.НоменклатураАртикул КАК PRODUCTIDBUYER,
               |	ВТ_Товары.Количество КАК Количество,
               |	ВТ_Товары.СтавкаНДС КАК СтавкаНДС,
               |	ВТ_Товары.Цена КАК Цена,
               |	ВТ_Товары.Сумма КАК Сумма,
               |	ВТ_Товары.НоменклатураПредставление КАК DESCRIPTION,
               |	ВТ_Товары.CODE КАК CODE,
               |	ВТ_Товары.ВалютаДокумента КАК CURRENCY,
               |	ЕСТЬNULL(ПоступлениеТовары.КоличествоПоступление, 0) КАК КоличествоДоставлено,
               |	ЕСТЬNULL(ПоступлениеТовары.ЕдиницаИзмеренияНаименование, ""шт"") КАК ЕдИзмДоставлено
               |ИЗ
               |	ВТ_Товары КАК ВТ_Товары
               |		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_ШтрихкодыМакс КАК ВТ_ШтрихкодыМакс
               |		ПО ВТ_Товары.Номенклатура = ВТ_ШтрихкодыМакс.Номенклатура
               |		ЛЕВОЕ СОЕДИНЕНИЕ ПоступлениеТовары КАК ПоступлениеТовары
               |		ПО ВТ_Товары.Номенклатура = ПоступлениеТовары.Номенклатура
               |			И ВТ_Товары.Цена = ПоступлениеТовары.Цена
               |
               |СГРУППИРОВАТЬ ПО
               |	ВТ_Товары.Номенклатура,
               |	ВТ_Товары.Сумма,
               |	ВТ_Товары.Ссылка,
               |	ВТ_Товары.ЕдИзмНаименование,
               |	ВТ_Товары.НомерСтроки,
               |	ВТ_Товары.НоменклатураПредставление,
               |	ВТ_Товары.Количество,
               |	ВТ_Товары.Цена,
               |	ВТ_Товары.СтавкаНДС,
               |	ВТ_Товары.НоменклатураАртикул,
               |	ЕСТЬNULL(ВТ_ШтрихкодыМакс.Штрихкод, ВТ_Товары.НоменклатураАртикул),
               |	ВТ_Товары.CODE,
               |	ВТ_Товары.ВалютаДокумента,
               |	ЕСТЬNULL(ПоступлениеТовары.КоличествоПоступление, 0),
               |	ЕСТЬNULL(ПоступлениеТовары.ЕдиницаИзмеренияНаименование, ""шт"")";
