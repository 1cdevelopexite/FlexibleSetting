		<Value xsi:type="xs:string">Запрос = Новый Запрос;
Запрос.Текст = "ВЫБРАТЬ
               |	ВозвратТоваровОтПокупателя.Ссылка КАК Ссылка,
               |	ВозвратТоваровОтПокупателя.Организация КАК Организация,
               |	ВозвратТоваровОтПокупателя.Контрагент КАК Контрагент,
               |	ВозвратТоваровОтПокупателя.Номер КАК NUMBER,
               |	ВозвратТоваровОтПокупателя.Дата КАК Дата,
               |	ЭКОМ_ЦепочкиДокументовПоставщика.RETANN.ФайлДата КАК ФайлДата,
               |	ЭКОМ_ЦепочкиДокументовПоставщика.RETANN.ФайлНомер КАК RETANNNUMBER,
               |	ЭКОМ_ЦепочкиДокументовПоставщика.RETANN.Дата КАК RETANNДата,
               |	ЭКОМ_ЦепочкиДокументовПоставщика.xmlОрганизация КАК SUPPLIER,
               |	ЭКОМ_ЦепочкиДокументовПоставщика.xmlОрганизация КАК SENDER,
               |	ЭКОМ_ЦепочкиДокументовПоставщика.xmlКонтрагент КАК BUYER,
               |	ЭКОМ_ЦепочкиДокументовПоставщика.xmlКонтрагент КАК RECIPIENT,
               |	ЭКОМ_ЦепочкиДокументовПоставщика.xmlТочкаДоставки КАК DELIVERYPLACE,
               |	""RETINS"" КАК ВидДокумента,
               |	""27"" КАК ACTION,
               |	ЭКОМ_ЦепочкиДокументовПоставщика.ORDERNUMBER КАК ORDERNUMBER,
               |	ЭКОМ_ЦепочкиДокументовПоставщика.ORDERDATE КАК ЗаказДата
               |ИЗ
               |	Документ.ВозвратТоваровОтПокупателя КАК ВозвратТоваровОтПокупателя
               |		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ЭКОМ_ЦепочкиДокументовПоставщика КАК ЭКОМ_ЦепочкиДокументовПоставщика
               |		ПО (ЭКОМ_ЦепочкиДокументовПоставщика.ВозвратнаяНакладная = ВозвратТоваровОтПокупателя.Ссылка)
               |ГДЕ
               |	ВозвратТоваровОтПокупателя.Ссылка В (&amp;Ссылка)";
	Запрос.УстановитьПараметр("Ссылка", Источник);
	Результат = Запрос.Выполнить().Выгрузить();
	Результат.Колонки.Добавить("DATE"             , Новый ОписаниеТипов("Строка"));
	Результат.Колонки.Добавить("RETANNDATE"       , Новый ОписаниеТипов("Строка"));
	Результат.Колонки.Добавить("RETURNDATE"       , Новый ОписаниеТипов("Строка"));
	Результат.Колонки.Добавить("ORDERDATE"        , Новый ОписаниеТипов("Строка"));
	Результат.Колонки.Добавить("EDIMESSAGE"       , Новый ОписаниеТипов("Строка"));
	Результат.Колонки.Добавить("EDIINTERCHANGEID" , Новый ОписаниеТипов("Строка"));
	Результат.Колонки.Добавить("ФайлИмя" 		  , Новый ОписаниеТипов("Строка"));
	Результат.Колонки.Добавить("Идентификатор"    , Новый ОписаниеТипов("Строка"));
	
	EDIINTERCHANGEID = 0;
	
	Для Каждого Строка Из Результат Цикл
	    // EDIMESSAGE
		EDIINTERCHANGEID = НовыйНомерТранзакции(EDIINTERCHANGEID);
	    Строка.EDIINTERCHANGEID = Формат(EDIINTERCHANGEID, "ЧГ=0");
	    ГСЧ                     = Новый ГенераторСлучайныхЧисел;
	    СлучЧисло               = ГСЧ.СлучайноеЧисло(100, 999);
	    Строка.EDIMESSAGE       = Формат(ТекущаяДата(), "ДФ=ггггММддЧЧммсс000") + СлучЧисло + "-OUT-" + Строка.EDIINTERCHANGEID;
	    Строка.ФайлИмя          = Строка.ВидДокумента + "_" + Строка.EDIMESSAGE + ".xml";
   		Строка.Идентификатор    = Строка.ВидДокумента + "_" + Строка.EDIMESSAGE;

		Строка.DATE        = Формат(Строка.Дата      , "ДФ=yyyy-MM-dd");
		Строка.RETANNDATE  = Формат(Строка.RETANNДата, "ДФ=yyyy-MM-dd");
		Строка.RETURNDATE  = Формат(Строка.ФайлДата  , "ДФ=yyyy-MM-dd");
		Строка.ORDERDATE  = Формат(Строка.ЗаказДата  , "ДФ=yyyy-MM-dd");
	КонецЦикла;
	
	Возврат Результат;
