		<Value xsi:type="xs:string">// Запишем документ в базу
Если Приемник.Товары.Количество() = 0 Тогда
	Сообщить("Документ ""Реализация товаров"" по ЭКОМ документу № " + Источник.Номер +
	" не создан, т.к. не заполнены товары, возможно документ был сформирован вручную, проверьте Реализацию к этому заказу.", СтатусСообщения.Важное);
Иначе
	Попытка
		Приемник.Записать(РежимЗаписиДокумента.Проведение);

		ЭКОМ_ДокументОбъект = Источник.ПолучитьОбъект();
		ЭКОМ_ДокументОбъект.ДокументыСвязанные.Очистить();
		Нов = ЭКОМ_ДокументОбъект.ДокументыСвязанные.Добавить();
		Нов.ДокументСвязанный = Приемник.Ссылка;
		ЭКОМ_ДокументОбъект.Записать();

		Сообщить("Поступление по ЭКОМ документу № " + Источник.Номер + " создано! Номер ПН № " + Приемник.Номер);
	Исключение
